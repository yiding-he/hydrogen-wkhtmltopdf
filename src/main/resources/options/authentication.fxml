<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="javafx" spacing="10" style="-fx-padding: 10"
      fx:controller="com.hyd.wkhtmltopdf.options.AuthenticationController">

    <HBox alignment="BASELINE_LEFT" spacing="10">
        <Label text="%lbl_auth_username"/>
        <TextField prefWidth="400" fx:id="txtAuthUserName"/>
    </HBox>

    <HBox alignment="BASELINE_LEFT" spacing="10">
        <Label text="%lbl_auth_password"/>
        <TextField prefWidth="400" fx:id="txtAuthPassword"/>
    </HBox>
</VBox>

